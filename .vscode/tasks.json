{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "make build",
      "type": "shell",
      "command": "make",
      "options": { "cwd": "${workspaceFolder}" },
      "group": "build",
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "make clean",
      "type": "shell",
      "command": "make clean",
      "options": { "cwd": "${workspaceFolder}" }
    }
  ]
}